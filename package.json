{
  "name": "honey-pager",
  "version": "0.0.0",
  "description": "A Relay-style cursor pagination tool for mongoose schema.",
  "main": "build/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint index.js lib/* graphql/*",
    "test": "yarn run lint && jest",
    "clean": "rm -rf build",
    "build": "yarn run clean && babel -d ./build --ignore node_modules,*test ."
  },
  "repository": "git@github.com:codeinbrain/honey-pager.git",
  "author": "Louis Bodart <lo.bodart3@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "build",
    "LICENSE",
    "README.MD"
  ],
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.3.0",
    "jest": "^23.5.0",
    "mongoose": "^5.2.7"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.17",
    "validator": "^10.5.0"
  }
}
